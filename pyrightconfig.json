{
    "include": [
        "llamachain"
    ],
    "exclude": [
        "**/node_modules",
        "**/__pycache__",
        "**/.venv",
        "**/venv"
    ],
    "ignore": [
        "llamachain/ignored_file.py"
    ],
    "defineConstant": {
        "DEBUG": true
    },
    "typeCheckingMode": "basic",
    "useLibraryCodeForTypes": true,
    "reportMissingImports": true,
    "reportMissingTypeStubs": false,
    "reportGeneralTypeIssues": "warning",
    "pythonVersion": "3.9",
    "pythonPlatform": "All",
    "stubPath": "stubs",
    "venvPath": ".",
    "venv": "venv"
}